root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
trim_trailing_whitespace = true

[*.out]
insert_final_newline = false
trim_trailing_whitespace = false

[*.go]
indent_size = 2
indent_style = space

[Makefile]
indent_size = 4
indent_style = tabs

[*.toml]
insert_final_newline = false
trim_trailing_whitespace = false
indent_size = 2
indent_style = space
